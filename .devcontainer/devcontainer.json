{
  "build": {
    "dockerfile" : "Dockerfile"
  },
  "hostRequirements": {
    "cpus": 4
  },
  "waitFor": "onCreateCommand",
  "postCreateCommand": "",
  "customizations": {
    "codespaces": {
      "openFiles": []
    },
    "vscode": {
      "extensions": [
        "ms-toolsai.jupyter",
        "ms-python.python",
        "ms-vsliveshare.vsliveshare"
      ],
      "settings": {"jupyter.kernels.trusted": ["/workspaces/codespaces-jupyter/.vscode/kernel_spec.json"]}
    }
  }
}
